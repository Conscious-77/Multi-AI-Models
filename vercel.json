{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://multi-ai-models-production.up.railway.app/api/$1" },
    { "source": "/((?!static/|favicon.ico|manifest.json|logo192.png|logo512.png).*)", "destination": "/" }
  ]
}
